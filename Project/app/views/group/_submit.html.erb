<%= form_for :groups do |f| %>
 

 
  <p>
    <%= f.label :groupName %><br>
    <%= f.text_field :groupName %>
  </p>
    <p>
    <%= f.label :numberNeeded %><br>
    <%= f.text_field :numberNeeded %>
  </p>
    <p>
    <%= f.label :numberTotal %><br>
    <%= f.text_field :numberTotal %>
  </p>
    
   <p>
    <%= f.label :category %><br>
    <%= f.collection_select :category, Category %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
 
<% end %>