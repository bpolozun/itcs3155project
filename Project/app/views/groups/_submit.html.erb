<%= form_for @group do |f| %>
 

 
  <p>
    <%= f.label :groupName %><br>
    <%= f.text_field :groupName %>
  </p>
  <p>
    <%= f.label :numberNeeded %><br>
    <%= f.text_field :numberNeeded %>
  </p>
  <p>
    <%= f.label :numberTotal %><br>
    <%= f.text_field :numberTotal %>
  </p>
    
  <p>
    <%= f.label :category %><br>
    <%= f.select :category, ['Sports', 'Extreme Sports', 'Music', 'Online Games', 'Academics', 'Other'] %>
    
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
 
<% end %>