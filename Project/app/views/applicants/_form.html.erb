    <%= form_for([@group, @group.applicants.build]) do |f| %>
        <p>
            <%= f.label :name %> <br>
            <%= f.text_field :name %>
        </p>
        
        <p>
            <%= f.label :contact %> <br>
            <%= f.text_field :contact %>
        </p>
        
        <p>
            <%= f.submit %>
        </p>
    <% end %>